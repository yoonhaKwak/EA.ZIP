<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bit.project.eazip.mapper.SampleMapper">

    <select id="selectData" resultType="bit.project.eazip.domain.SampleDTO">
        SELECT * from hospital_tbl;
    </select>

    <select id="selectList" resultType="bit.project.eazip.domain.SampleDTO">
        select * from hospital_tbl where name like "%서울%";
    </select>

    <select id="selectLat" resultType="bit.project.eazip.domain.SampleDTO">
        select lat from hospital_tbl;
    </select>

    <select id="selectLng" resultType="bit.project.eazip.domain.SampleDTO">
        select lng from hospital_tbl;
    </select>
</mapper>