<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bit.project.eazip.mapper.mypage.MypageMapper">

    <select id="getList" resultType="bit.project.eazip.domain.home.HomeDTO">
        SELECT * from gangnam0712 WHERE idx IN
        <foreach collection="array" item="arr" open="(" close=")" separator=",">
            #{arr}
        </foreach>
    </select>



    <select id="getIdx" resultType="String">
        SELECT history from usertbl WHERE id =  '2361494735'
    </select>

    <insert id="insertHistory">
        update usertbl set history =
                               #{temp}
        where id = '2361494735'
    </insert>


</mapper>